{
  "suggest.noselect": false,
  "solargraph.commandPath": "/usr/local/bin/solargraph",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".git/",
        ".env"
      ],
      "filetypes": [
        "go"
      ],
      "disableWorkspaceFolders": true
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "python",
    "php"
  ],
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "php": "html",
    "blade": "html"
  },
  "bladeFormatter.format.indentSize": 2,
  "bladeFormatter.format.wrapLineLength": 200,
  "vetur.ignoreProjectWarning": true,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": false,
  "explorer.buffer.root.template": "[icon & 1] OPEN EDITORS",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.file.showHiddenFiles": true,
  "explorer.file.root.template": "[icon & 1] PROJECT ([root])",
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "vim-devicons"
}
